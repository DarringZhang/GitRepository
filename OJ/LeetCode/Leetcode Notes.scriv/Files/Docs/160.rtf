\par\plain \ilvl0{\listtext\f1\fs26\b0\i0	\loch\af1\hich\af1\dbch\af1\uc1\u9679\'3F	}{\f1\fs26\b0\i0 DOCX}
\par\plain \ilvl0{\listtext\f1\fs26\b0\i0	\loch\af1\hich\af1\dbch\af1\uc1\u9679\'3F	}{\f1\fs26\b0\i0 ODT}
\par\plain \ilvl0{\listtext\f1\fs26\b0\i0	\loch\af1\hich\af1\dbch\af1\uc1\u9679\'3F	}{\f1\fs26\b0\i0 TXT}
\par\plain \ilvl0{\listtext\f1\fs26\b0\i0	\loch\af1\hich\af1\dbch\af1\uc1\u9679\'3F	}{\f1\fs26\b0\i0 PDF}
